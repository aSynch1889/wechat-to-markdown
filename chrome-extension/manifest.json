{
    "manifest_version": 3,
    "name": "微信公众号转Markdown",
    "version": "1.0",
    "description": "一键将微信公众号文章转换为Markdown格式",
"permissions": [
  "activeTab",
  "scripting",
  "downloads"
],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }